i = 1
j = 1
while i<6:
    while j<6:
        if i%2==0 and j%2==0:
            print(" ",end="")
        else:
            print("*",end="")
        j+=1
    print()
    j=1
    i+=1
print()
a=0
b=0
while a<5:
    while b<5:
        if a==b:
            print(" ",end="")
        else:
            print("*",end="")
        b+=1
    print()
    b=0
    a+=1
