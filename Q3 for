for i in range(1,6):
    for k in range(1,6):
        if i%2==0 and k%2==0:
            print(" ",end="")
        else:
            print("*",end="")
    print()
print()

for a in range(5):
    for b in range(5):
        if a==b:
            print(" ",end="")
        else:
            print("*",end="")
    print()